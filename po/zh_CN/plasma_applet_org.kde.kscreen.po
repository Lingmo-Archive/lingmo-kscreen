msgid "Advanced Display Settings"
msgstr "高级显示设置"

#: package/contents/ui/PresentationModeItem.qml:39
#, kde-format
msgid "Enable Presentation Mode"
msgstr "启用演示模式"

#: package/contents/ui/PresentationModeItem.qml:53
